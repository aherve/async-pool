{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 250,
  "singleAttributePerLine": true,
  "overrides": [
    {
      "files": "*.ts*",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
